<?xml version="1.0" encoding="UTF-8"?>
<grid>
	<config title="流程审批错误信息" base="/core/ord/err/" xtype="uxgrid" curd="CRD"/>
	<form>
		<column xtype="textfield" fieldLabel="流程实例ID" name="procinstid" allowBlank="false"/>
		<column xtype="textfield" fieldLabel="流程定义ID" name="prodefid" allowBlank="false"/>
		<column xtype="textfield" fieldLabel="任务ID" name="taskid" allowBlank="false"/>
		<column xtype="textfield" fieldLabel="任务定义ID" hidden="true" name="executionId" allowBlank="false"/>
		<column xtype="textfield" fieldLabel="发现环节" name="taskname" allowBlank="false"/>
		<column xtype="textfield" fieldLabel="出错原因" name="errType" allowBlank="false"/>
		<column xtype="textfield" fieldLabel="原因描述" name="errDesc"/>
		<column xtype="textfield" fieldLabel="当前任务节点" name="taskDefinitionKey"/>
		<column xtype="textfield" fieldLabel="目标任务节点" name="targetTaskDefinitionKey"/>
		<column xtype="fieldset" title="系统信息">
			<column xtype="textfield" fieldLabel="UUID" name="id"  readOnly="true" fieldStyle="background:#E6E6E6"/>
			<column xtype="textfield" fieldLabel="创建人" name="createUser"  readOnly="true" fieldStyle="background:#E6E6E6"/>
			<column xtype="datefield" fieldLabel="创建时间" name="createTime" format="Y-m-d H:i:s" fieldStyle="background:#E6E6E6" readOnly="true"/>
			<column xtype="textfield" fieldLabel="更新人" name="updateUser"  readOnly="true" fieldStyle="background:#E6E6E6"/>
			<column xtype="datefield" fieldLabel="更新时间" name="updateTime" format="Y-m-d H:i:s" fieldStyle="background:#E6E6E6" readOnly="true"/>
		</column>
	</form>
 
	<search>
		<column layout="hbox" xtype="fieldcontainer">
			<column xtype="container" flex="1" layout="hbox">
				<column xtype="textfield" fieldLabel="订单号" name="zuonr"/>
			</column>
		</column>
		<column layout="hbox" xtype="fieldcontainer">
			<column xtype="container" flex="1" layout="hbox">
				<column xtype="textfield" fieldLabel="创建人" name="createUser"  />
				<column xtype="datefield" fieldLabel="新增日期[S]" name="createTime" format="Y-m-d"/>
				<column xtype="datefield" fieldLabel="新增日期[E]" name="createTime" format="Y-m-d"/>
			</column>
		</column>
	</search>
	<head>
		<column xtype="rownumberer" width="30" align="right"/>
		<column dataIndex="procinstid" width="120" align="left" text="流程实例ID"  hidden="true" />
		<column dataIndex="prodefid" width="120" align="left" text="流程定义ID"  hidden="true" />
		<column dataIndex="executionId" width="120" align="left" text="任务定义ID"  hidden="true" />
		<column dataIndex="taskid" width="120" align="left" text="任务ID"  hidden="true" />
		<column dataIndex="mappingId" width="120" align="left" text="关联Id" hidden="true" />
		<column dataIndex="mappingSid" width="120" align="left" text="关联单号"/>
		<column dataIndex="taskname" width="120" align="left" text="发现环节"/>
		<column dataIndex="targetTaskName" width="120" align="left" text="出错环节"/>
		<column dataIndex="errType" width="120" align="left" text="出错原因" flex="1">
			<editor xtype="dictcombobox" dict="ERROR_ORD_TYPE" editable="false"/>
		</column>
		<column dataIndex="errDesc" width="120" align="left" text="原因描述"/>
		<column dataIndex="createUser"  width="100" align="center" text="发现人" />
		<column dataIndex="taskDefinitionKey" width="120" align="left" text="当前任务节点" hidden="true" />
		<column dataIndex="targetTaskDefinitionKey" width="120" align="left" text="目标任务节点" hidden="true" />
		<column dataIndex="createTime" xtype="datecolumn" width="150" align="center" text="创建时间" format="Y-m-d H:i:s"/>
		<column dataIndex="updateTime" xtype="datecolumn" width="150" align="center" text="更新时间" format="Y-m-d H:i:s" hidden="true" />
	
	</head>
	<sql vname="ACT_CT_ORD_ERR">
	<![CDATA[ SELECT * FROM ACT_CT_ORD_ERR WHERE 1=1]]>
	</sql>
</grid>