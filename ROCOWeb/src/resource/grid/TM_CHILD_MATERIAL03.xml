<?xml version="1.0" encoding="UTF-8"?>
<grid>
	<config curdtname="IMOS_IDBEXT" updatedesc="orderid,id,typ" isPage="false" insertRow="orderid,typ,name,thickness,cthickness,width,cwidth,length,clength,cnt,matname,idSerie,grid,info4" needLog="true"/>
	<validations>
		<column field="id" regexp="presence"/>
		<column field="name" regexp="presence"/>
		<column field="matname" regexp="presence"/>
		<column field="idSerie" regexp="presence"/>
		<column field="parentid" regexp="presence"/>
		<column field="info1" regexp="startAG6"/>
		<column field="grid" regexp="presence"/>
		<column field="info4" regexp="presence"/>
	</validations>
	<head>
		<column xtype="rownumberer" width="40" align="right"/>
		<column text="编号" dataIndex="id" width="80" align="left">
			<editor xtype="textfield" selectOnFocus="true"/>
		</column>
		<column text="名称" dataIndex="name" width="200" align="left">
			<editor xtype="textfield" selectOnFocus="true"/>
		</column>
		<column text="完工厚度" dataIndex="thickness" width="80" align="right">
			<editor xtype="numberfield" selectOnFocus="true" mouseWheelEnabled="false"/>
		</column>
		<column text="开料厚度" dataIndex="cthickness" width="80" align="right">
			<editor xtype="numberfield" selectOnFocus="true" mouseWheelEnabled="false"/>
		</column>
		<column text="完工宽度" dataIndex="width" width="80" align="right">
			<editor xtype="numberfield" selectOnFocus="true" mouseWheelEnabled="false"/>
		</column>
		<column text="开料宽度" dataIndex="cwidth" width="80" align="right">
			<editor xtype="numberfield" selectOnFocus="true" mouseWheelEnabled="false"/>
		</column>
		<column text="完工长度" dataIndex="length" width="80" align="right">
			<editor xtype="numberfield" selectOnFocus="true" mouseWheelEnabled="false"/>
		</column>
		<column text="开料长度" dataIndex="clength" width="80" align="right">
			<editor xtype="numberfield" selectOnFocus="true" mouseWheelEnabled="false"/>
		</column>
		
		<column text="数量" dataIndex="cnt" width="60" align="right">
		</column>
		<column text="原材料描述" dataIndex="matname" width="200" align="left">
			<editor xtype="textfield" selectOnFocus="true"/>
		</column>
		<column text="工艺路线" dataIndex="idSerie" width="100" align="left">
			<editor xtype="textfield" selectOnFocus="true"/>
		</column>
		<column text="SURFTNAM" dataIndex="surftnam" width="150" align="left">
			<editor xtype="textfield" selectOnFocus="true"/>
		</column>
		<column text="父编号" dataIndex="parentid" width="80" align="left">
			<editor xtype="textfield" selectOnFocus="true"/>
		</column>
		<column text="识别码" dataIndex="info1" width="60" align="left">
			<editor xtype="textfield" selectOnFocus="true"/>
		</column>
		<column text="订单编号" dataIndex="orderid" width="150" align="left" hidden="true"/>
		<column text="类型" dataIndex="typ" width="40" align="left" hidden="true"/>
		
		<column text="条形码" dataIndex="barcode" width="180" align="left">
			<editor xtype="textfield" selectOnFocus="true"/>
		</column>
		<column text="流水号" dataIndex="ncno" width="60" align="left">
			<editor xtype="textfield" selectOnFocus="true"/>
		</column>
		<column text="正面条码" dataIndex="cncBarcode1" width="180" align="left">
			<editor xtype="textfield" selectOnFocus="true"/>
		</column>
		<column text="反面条码" dataIndex="cncBarcode2" width="180" align="left">
			<editor xtype="textfield" selectOnFocus="true"/>
		</column>
		<column text="纹理" dataIndex="grid" width="40" align="left">
			<editor xtype="textfield" selectOnFocus="true"/>
		</column>
		<column text="加工说明" dataIndex="idText" width="100" align="left">
			<editor xtype="textfield" selectOnFocus="true"/>
		</column>
		<column text="预留字段2" dataIndex="info2" width="100" align="left">
			<editor xtype="textfield" selectOnFocus="true"/>
		</column>
		<column text="预留字段3" dataIndex="info3" width="100" align="left">
			<editor xtype="textfield" selectOnFocus="true"/>
		</column>
		<column text="旧工艺路线" dataIndex="info4" width="100" align="left">
			<editor xtype="textfield" selectOnFocus="true"/>
		</column>
		<column text="板件标准码1" dataIndex="info5" width="100" align="left">
			<editor xtype="textfield" selectOnFocus="true"/>
		</column>
		<column text="板件标准码2" dataIndex="checksum2" width="100" align="left">
			<editor xtype="textfield" selectOnFocus="true"/>
		</column>
		<column text="功能件标准码" dataIndex="checksum" width="100" align="left">
			<editor xtype="textfield" selectOnFocus="true"/>
		</column>
	</head>
	<sql vname="tree_material03">
	<![CDATA[select id,parentid,orderid,typ,name2,name,cthickness,clength,cwidth,length,width,thickness,cnt,barcode,ncno,cnc_Barcode1,cnc_Barcode2,grid,id_Serie,id_Text,info1,info2,info3,info4,info5,checksum2,checksum,matname,surftnam from IMOS_IDBEXT where typ='3' and info1 like 'E%' ]]>
	</sql>
</grid>