<?xml version="1.0" encoding="UTF-8"?>
<grid>
	<config title="手动分配任务流水" base="/core/bpm/tasklog/" xtype="uxgrid" selModelStatus="false" gridGroupField="createUser"  curd="R"/>
	<search>
		<column layout="hbox" xtype="fieldcontainer">
			<column xtype="container" flex="1" layout="hbox">
				<column xtype="textfield" fieldLabel="订单号" name="orderCode"/>
				<column xtype="tablecombobox" fieldLabel="售达方" name="shouDaFang" table="cust_header" dataid="kunnr" datatext="name1" showKey="true"/>
				<column xtype="textfield" fieldLabel="处理人" name="assignee"/>
				<column xtype="tablecombobox" fieldLabel="任务名称" name="groupId" table="SYS_GROUP" dataid="ID" datatext="NAME" showKey="false"/>
			</column>
		</column>
		<column layout="hbox" xtype="fieldcontainer">
			<column xtype="container" flex="1" layout="hbox">
				<column xtype="textfield" fieldLabel="指派人" name="createUser"/>
				<column xtype="textfield" fieldLabel="售达方名称" name="name1" />
				<column xtype="datefield" fieldLabel="新增日期[S]" name="createTime" format="Y-m-d"/>
				<column xtype="datefield" fieldLabel="新增日期[E]" name="createTime" format="Y-m-d"/>
			</column>
		</column>
	</search>
	<head>
		<column xtype="rownumberer" width="30" align="right"/>
		<column dataIndex="orderCode" width="140" align="left" text="订单号">
			<editor xtype="textfield" selectOnFocus="true"/>
			<filter type="string"/>
		</column>
		<column dataIndex="orderType" width="80" align="center" text="订单类型">
			<editor xtype="dictcombobox" dict="ORDER_TYPE" editable="false"/>
		</column>
		<column dataIndex="orderDate" xtype="datecolumn" width="100" align="center" text="订单日期" format="Y-m-d"/>
		<column dataIndex="shouDaFang" width="70" align="center"  text="售达方" filterable="true">
			<editor xtype="textfield" selectOnFocus="true"/>
			<filter type="string"/>
		</column>
		<column dataIndex="name1" width="80" align="center"  text="售达方名称" filterable="true">
			<editor xtype="textfield" selectOnFocus="true"/>
			<filter type="string"/>
		</column>
		<column dataIndex="assignee" width="100" align="center" text="处理人" filterable="true">
			<editor xtype="textfield" selectOnFocus="true"/>
			<filter type="string"/>
		</column>
		<column dataIndex="oldAssignee" width="100" align="center"  text="旧处理人" filterable="true">
			<editor xtype="textfield" selectOnFocus="true"/>
			<filter type="string"/>
		</column>
		<column dataIndex="taskNo" width="70" align="center" text="任务编号" filterable="true">
			<editor xtype="textfield" selectOnFocus="true"/>
			<filter type="string"/>
		</column>
		<column dataIndex="taskName" width="75" align="left" text="任务名称" filterable="true">
			<editor xtype="textfield" selectOnFocus="true"/>
			<filter type="string"/>
		</column>
		<column dataIndex="createUser" width="100" align="center"  text="指派人" filterable="true">
			<editor xtype="textfield" selectOnFocus="true"/>
			<filter type="string"/>
		</column>
		<column dataIndex="createTime" xtype="datecolumn" width="140" align="center" text="创建时间" format="Y-m-d H:i:s"/>
		<column dataIndex="taskId" width="120" align="center" hidden="true" text="任务标识" filterable="true">
			<editor xtype="textfield" selectOnFocus="true"/>
			<filter type="string"/>
		</column>
		<column dataIndex="groupId" width="130" align="center" hidden="true" text="任务分组" filterable="true">
			<editor xtype="textfield" selectOnFocus="true"/>
			<filter type="string"/>
		</column>
		<column dataIndex="proDefId" width="220" align="center" hidden="true" text="版本" filterable="true">
			<editor xtype="textfield" selectOnFocus="true"/>
			<filter type="string"/>
		</column>
	</head>
	<sql vname="ACT_CT_TASK_LOG">
	<![CDATA[select * from ACT_CT_TASK_LOG where 1=1 ]]>
	</sql>
</grid>