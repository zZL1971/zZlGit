<?xml version="1.0" encoding="UTF-8"?>
<grid>
	<config title="遗留订单收费明细表(店面单号)" base="/main/sd/pr/" xtype="uxgrid"
		curd="RE" gridGroupField="createUser" isDateNec="false" />
	<search>
		<column layout="hbox" xtype="fieldcontainer">
			<column xtype="container" flex="1" layout="hbox">
				<column xtype="textfield" fieldLabel="订单编号" name="orderCode"
					editable="false" />
				<column xtype="tablecombobox" fieldLabel="售达方" name="shouDaFang"
					table="cust_header" dataid="kunnr" datatext="name1" showKey="true" />
				<column xtype="dictcombobox" fieldLabel="订单类型" name="orderType"
					dict="ORDER_TYPE" />
				<column xtype="dictcombobox" fieldLabel="遗留天数" name="isStay"
					dict="STAY_DAY" />
				<column xtype="dictcombobox" fieldLabel="处理状态" name="stayStatus"
					dict="STAY_STATUS" />
			</column>
		</column>
		<column layout="hbox" xtype="fieldcontainer">
			<column xtype="container" flex="1" layout="hbox">
				<column xtype="dictcombobox" fieldLabel="是否样板" name="isYp"
					dict="T_OR_F" />
				<column xtype="dictcombobox" fieldLabel="地区" name="regio"
					dict="REGIO" />
				<column xtype="datefield" fieldLabel="确认报价开始时间" name="startTime"
					format="Y-m-d" />
				<column xtype="datefield" fieldLabel="到" name="startTime"
					format="Y-m-d" />
				<column xtype="dictcombobox" fieldLabel="是否取消" name="orderStatus"
					dict="T_OR_F" />
			</column>
		</column>
	</search>
	<head>
		<column xtype="rownumberer" width="30" align="right" />
		<column dataIndex="regio" width="70" align="left" text="地区">
			<editor xtype="dictcombobox" dict="KUNNR_REGION" editable="false" />
		</column>
		<column dataIndex="name1" width="90" align="center" text="售达方名称" />
		<column dataIndex="orderType" width="110" align="center" text="订单类型">
			<editor xtype="dictcombobox" dict="ORDER_TYPE" editable="false" />
		</column>
		<column dataIndex="orderCode" width="130" align="center" text="订单编号" />
		<column dataIndex="isYp" width="80" align="center" text="是否样品">
			<editor xtype="dictcombobox" dict="T_OR_F" editable="false" />
		</column>
		<column dataIndex="orderDate" xtype="datecolumn" width="110"
			align="center" text="订单日期" format="Y-m-d" />
		<column dataIndex="name" width="130" align="center" text="当前环节" />
		<column dataIndex="startTime" xtype="datecolumn" width="150"
			align="center" text="确认报价开始时间" format="Y-m-d H:i:s" />
		<column dataIndex="endTime" xtype="datecolumn" width="150"
			align="center" text="确认报价结束时间" format="Y-m-d H:i:s" />
		<column dataIndex="day" width="90" align="center" text="遗留天数" />
		<column dataIndex="orderStatus" width="80" text="是否取消" align="center">
			<editor xtype="dictcombobox" dict="T_OR_F" editable="false" />
		</column>
		<column dataIndex="stayStatus" width="80" align="center" text="处理状态">
			<editor xtype="dictcombobox" dict="STAY_STATUS" editable="false" />
		</column>
	</head>
	<sql vname="Z_RP_STAYORD_PAY">
	<![CDATA[select * from Z_RP_STAYORD_PAY where 1=1]]>
	</sql>
</grid>