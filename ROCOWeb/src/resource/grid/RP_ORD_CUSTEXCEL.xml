<?xml version="1.0" encoding="UTF-8"?>
<grid>
	<config title="客户信息导出" base="/main/sd/pr/"  xtype="uxgrid" selModelStatus="false" gridGroupField="createUser" isDateNec="true" curd="RE"/>
	<search>
		<column layout="hbox" xtype="fieldcontainer">
			<column xtype="container" flex="1" layout="hbox">
				<column xtype="datefield" fieldLabel="交易日期从" name="createTime"
					format="Y-m-d" />
				<column xtype="datefield" fieldLabel="到" name="createTime"
					format="Y-m-d" />
