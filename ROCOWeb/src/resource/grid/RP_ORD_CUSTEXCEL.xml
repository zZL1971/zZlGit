		<column dataIndex="jrnNo" width="140" align="center" text="销售区域" />
		<column dataIndex="xinDai" width="110" align="center" text="信贷额度" />
    	<column dataIndex="kunnrS" width="80" align="center" text="送达方" />
		<!-- <column dataIndex="tradeAmount" width="140" align="center" text="折扣" />
		<column dataIndex="kunnr" width="100" align="center" text="是否启用" /> -->
	</head>
