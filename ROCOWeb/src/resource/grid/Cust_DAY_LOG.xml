<?xml version="1.0" encoding="UTF-8"?>
<grid>
	<config title="客服产能及质量报表" base="/main/sd/pr/" xtype="uxgrid"  curd="RE"/>
	<search>
		<column layout="hbox" xtype="fieldcontainer">
			<column xtype="container" flex="1" layout="hbox">
				<column xtype="datefield" fieldLabel="创建日期" name="ytime"
					format="Y-m-d" />
				<column xtype="datefield" fieldLabel="到" name="ytime"
					format="Y-m-d" />
				<column xtype="textfield" fieldLabel="姓名" name="assignee" />
			</column>
		</column>
	</search>
	
	<head>
		<column xtype="rownumberer" width="40" align="right"/>
		<column dataIndex="ytime" xtype="datecolumn" width="145" align="center" text="日期" format="Y-m-d"/>
		<column dataIndex="assignee" width="100" align="left" text="姓名" />
		<column dataIndex="cccount"  width="120" align="left" text="订单审绘退单数量" />
		<column dataIndex="zcount"  width="120" align="left" text="客服审核订单数" />
		<column dataIndex="zlhegl"  width="120" align="left" text="质量合格率" />
	</head>
	<sql vname="RP_ORD_KFCN">
	<![CDATA[
		select * from RP_ORD_KFCN
	]]>
	<format name="ytime" dateFormat="yyyy-MM-dd"/>
	</sql>
</grid>