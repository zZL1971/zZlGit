<?xml version="1.0" encoding="UTF-8"?>
<grid>
	<config title="订单进度报表" base="/main/sd/pr/" xtype="uxgrid" curd="RE" selModelStatus="false" gridGroupField="createUser" isDateNec="true"/>
	<search>
	<column layout="hbox" xtype="fieldcontainer">
			<column xtype="container" flex="1" layout="hbox">
				<column xtype="datefield" fieldLabel="创建日期" name="orderDate"
					format="Y-m-d" />
				<column xtype="datefield" fieldLabel="到" name="orderDate"
					format="Y-m-d" />
				<column xtype="dictcombobox" fieldLabel="订单类型" name="orderTypes" localData='["","补购订单","免费订单"]' />
			</column>
		</column>
	</search>
	<head>
		<column xtype="rownumberer" width="30" align="right" />
		<column dataIndex="orderDate" xtype="datecolumn" width="145" align="center" text="日期" format="Y-m-d"/>
		<column dataIndex="orderType" width="100" align="left" text="订单类型" />
		<column dataIndex="zccount" width="100" align="left" text="客服下单" summaryType="sum"/>
		<column dataIndex="khqc"  width="120" align="left" text="客户起草(补单)" summaryType="sum"/>
		<column dataIndex="kfsh"  width="120" align="left" text="客服审核" summaryType="sum"/>
		<column dataIndex="ddsh"  width="120" align="left" text="订单审绘" summaryType="sum"/>
		<column dataIndex="zd"  width="120" align="left" text="炸单(外部)" summaryType="sum"/>
		<column dataIndex="wlsh"  width="120" align="left" text="物料审核" summaryType="sum"/>
		<column dataIndex="jgsh"  width="120" align="left" text="价格审核" summaryType="sum"/>
		<column dataIndex="khqr"  width="120" align="left" text="客户确认" summaryType="sum"/>
		<column dataIndex="cwqr"  width="120" align="left" text="财务确认" summaryType="sum"/>
		<column dataIndex="js"  width="120" align="left" text="结束" summaryType="sum"/>
	</head>
	<sql vname="Cust_Link_Num_New">
	<![CDATA[ select * from cust_link_num ]]>
	<format name="orderDate" dateFormat="yyyy-MM-dd"/>
	</sql>
</grid>