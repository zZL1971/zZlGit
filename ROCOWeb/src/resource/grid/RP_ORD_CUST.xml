<?xml version="1.0" encoding="UTF-8"?>
<grid>
	<config title="客户汇款记录" base="/main/sd/pr/"  xtype="uxgrid" selModelStatus="false" gridGroupField="createUser" isDateNec="true" curd="RE"/>
	<search>
		<column layout="hbox" xtype="fieldcontainer">
			<column xtype="container" flex="1" layout="hbox">
				<column xtype="datefield" fieldLabel="交易日期从" name="tradeTime"
					format="Y-m-d" />
				<column xtype="datefield" fieldLabel="到" name="tradeTime"
					format="Y-m-d" />
				<column xtype="dictcombobox" fieldLabel="选择银行" name="statusNum"
					dict="BANK" />
				<column xtype="textfield" fieldLabel="付款人" name="payer" editable="false"/>
				<column xtype="textfield" fieldLabel="客户编码" name="kunnr" editable="false"/>
				
				
			</column>
		</column>
	</search>
	<head>
		<column xtype="rownumberer" width="35" align="right" />
		<column dataIndex="shid" hidden="true" name="shid" />
		<column dataIndex="tradeCompanyId" width="140" align="center" text="付款方企业编号" />
		<column dataIndex="resumo" width="140" align="center" text="凭证" />
		<column dataIndex="payer" width="140" align="center" text="付款人" />
		<column dataIndex="tradeAccNo" width="250" align="center"  text="付款帐号" />
		<column dataIndex="bankId" width="250" align="center"  text="绑定帐号" />
		<column dataIndex="statusNum" width="60" align="center" text="银行种类">
			<editor xtype="dictcombobox" dict="BANK" editable="false" />
		</column>
		<column dataIndex="jrnNo" width="140" align="center" text="交易日志号" />
		<!-- <column dataIndex="tradeDate" width="110" align="center" text="交易日期" /> -->
 		<column dataIndex="tradeTime" xtype="datecolumn" width="140" align="center" text="交易日期" format="Y-m-d H:i:s"/>
		<!-- <column dataIndex="dealerNum" width="80" align="center" text="经销商编号" /> -->
		<column dataIndex="tradeAmount" width="140" align="center" text="金额" />
		<column dataIndex="kunnr" width="100" align="center" text="店面" />
		<column dataIndex="accName1" width="150" align="center" text="打款人" />
		<column dataIndex="accno2" width="200" align="center" text="打款账号" />
	</head>
	<sql vname="RP_ORD_CUST">
	<![CDATA[select * from RP_ORD_CUST where 1=1]]>
	</sql>
</grid>