<?xml version="1.0" encoding="UTF-8"?>
<grid>
	<config title="客户折扣报表" base="/main/sd/pr/" xtype="uxgrid" curd="RE"/>
	<search>
		<column layout="hbox" xtype="fieldcontainer">
			<column xtype="container" flex="1" layout="hbox">
				<column xtype="dictcombobox" fieldLabel="查询年份" name="year" dict="PYEAR" />
				<column xtype="dictcombobox" fieldLabel="地区" name="areaId" dict="KUNNR_REGION"  />
				<column xtype="textfield" fieldLabel="售达方" name="storeId" />
				<column xtype="textfield" fieldLabel="售达方名称" name="subArea" />
			</column>
		</column>
	</search>
	<head>
		<column xtype="rownumberer" width="30" align="right"/>
		<column dataIndex="area" width="100" align="center" text="地区"/>
		<column dataIndex="subArea" width="100" align="center" text="售达方名称"/>
		<column dataIndex="storeId" width="100" align="center" text="售达方"/>
		<column dataIndex="status" width="80" align="center" text="状态"/>
		<column dataIndex="expired" width="80" align="center" text="是否过期"/>
		<column dataIndex="year" width="80" align="center" text="年份"/>
		
		<column dataIndex="startDate" width="150" align="left" text="开始日期" xtype="datecolumn" format="Y-m-d" />
		<column dataIndex="endDate"  width="150" align="left" text="结束日期" xtype="datecolumn" format="Y-m-d"/>
		
		<column dataIndex="fdName" width="80" align="center" text="返点类型"/>
		<column dataIndex="discount" width="80" align="right" text="返点折扣"/>
		<column dataIndex="total" width="100" align="right" text="返点金额" xtype="numbercolumn" format="0.00" />
		<column dataIndex="surplus" width="100" align="right" text="未返点金额" xtype="numbercolumn" format="0.00" />		
		<column dataIndex="totalToyear" width="100" align="right" text="本年返还金额" xtype="numbercolumn" format="0.00"/>
		<column dataIndex="jan" width="90" align="right" text="1月份" xtype="numbercolumn" format="0.00"/>
		<column dataIndex="feb" width="90" align="right" text="2月份" xtype="numbercolumn" format="0.00"/>	
		<column dataIndex="mar" width="90" align="right" text="3月份"  xtype="numbercolumn" format="0.00"/>	
		<column dataIndex="apr" width="90" align="right" text="4月份"  xtype="numbercolumn" format="0.00"/>	
		<column dataIndex="may" width="90" align="right" text="5月份"  xtype="numbercolumn" format="0.00"/>	
		<column dataIndex="jun" width="90" align="right" text="6月份"  xtype="numbercolumn" format="0.00"/>	
		<column dataIndex="jul" width="90" align="right" text="7月份"  xtype="numbercolumn" format="0.00"/>	
		<column dataIndex="aug" width="90" align="right" text="8月份"  xtype="numbercolumn" format="0.00"/>	
		<column dataIndex="sept" width="90" align="right" text="9月份"  xtype="numbercolumn" format="0.00"/>	
		<column dataIndex="oct" width="90" align="right" text="10月份"  xtype="numbercolumn" format="0.00"/>	
		<column dataIndex="nov" width="90" align="right" text="11月份"  xtype="numbercolumn" format="0.00"/>	
		<column dataIndex="dec" width="90" align="right" text="12月份"  xtype="numbercolumn" format="0.00"/>
					 
	</head>
	<sql vname="RP_CUS_DIS">
	<![CDATA[select * from cust_discount_report_view_test where 1=1 ]]>
	</sql>
</grid>