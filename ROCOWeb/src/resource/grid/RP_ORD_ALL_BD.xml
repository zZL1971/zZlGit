<?xml version="1.0" encoding="UTF-8"?>
<grid>
	<config title="补单一览表" base="/main/sd/pr/" xtype="uxgrid" curd="RE" />
	<search>
		<column layout="hbox" xtype="fieldcontainer">
			<column xtype="container" flex="1" layout="hbox">
				<column xtype="textfield" fieldLabel="订单编号" name="orderCode"/>
				<column xtype="dictcombobox" fieldLabel="订单类型" name="orderType" localData='["","补购订单","免费订单"]' />	
				<column xtype="datefield" fieldLabel="创建日期" name="khqcend"
					format="Y-m-d" />
				<column xtype="datefield" fieldLabel="到" name="khqcend"
					format="Y-m-d" />
				<column xtype="textfield" fieldLabel="处理人" name="kfshclr"/>	
			</column>
		</column>
	</search>
	<head>
		<column xtype="rownumberer" width="30" align="right" />
		<column dataIndex="kunnr"  width="150" align="center" text="店面"/>
		<column dataIndex="orderCode" width="150" align="center" text="订单号"/>
		<column dataIndex="orderType" width="150" align="center" text="订单类型"/>
		<column dataIndex="khqcend" xtype="datecolumn" width="150" align="center" text="客户下单日期" format="Y-m-d H:i:s"/>
		<column dataIndex="kfshend" xtype="datecolumn" width="150" align="center" text="客服处理日期" format="Y-m-d H:i:s"/>
		<column dataIndex="kfsh" width="150" align="center" text="客服处理时长（分）"/>
		<column dataIndex="kfjg" width="150" align="center" text="客服处理时长是否延期"/>
		<column dataIndex="kfshclr" width="150" align="center" text="处理人"/>
		<column dataIndex="ddsh" width="150" align="center" text="订单审绘时长（分）"/>
		<column dataIndex="ddshjg" width="150" align="center" text="订单审绘处理时长是否延期"/>
		<column dataIndex="wlsh" width="150" align="center" text="订单审核时长（分）"/>
		<column dataIndex="wlshjg" width="150" align="center" text="订单审核时长是否延期"/>
		<column dataIndex="jgsh" width="150" align="center" text="订单审价时长（分）"/>
		<column dataIndex="jgshjg" width="150" align="center" text="订单审价时长是否延期"/>
		<column dataIndex="khqr" width="150" align="center" text="客户确认时长（分）"/>
		<column dataIndex="cwqr" width="150" align="center" text="财务确认时长（分）"/>
		
	</head>
	<sql vname="RP_ORD_ALL_BD">
	<![CDATA[
		select * from RP_ORD_ALL_BD where 1=1
	]]>
	</sql>
</grid>