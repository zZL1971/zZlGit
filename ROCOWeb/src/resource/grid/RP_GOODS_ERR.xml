<?xml version="1.0" encoding="UTF-8"?>
<grid>
	<config title="商品出错进度 报表" base="/main/sd/pr/" xtype="uxgrid" selModelStatus="false" gridGroupField="createUser"  isDateNec="true" curd="RE"/>
	<search>
		<column layout="hbox" xtype="fieldcontainer">
			<column xtype="container" flex="1" layout="hbox">
				<column xtype="textfield" fieldLabel="订单编号" name="orderCode" />
				<column xtype="datefield" fieldLabel="订单日期" name="orderDate" format="Y-m-d" />
				<column xtype="tablecombobox" fieldLabel="售达方" name="shouDaFang" table="cust_header" dataid="kunnr" datatext="name1" showKey="true"/>
				<column xtype="textfield" fieldLabel="售达方名称" name="kunnrName1" />
				<column xtype="dictcombobox" fieldLabel="开罚单" name="tackit" format="Y-m-d" dict="YES_NO"/>
			</column>
		</column>
		<column layout="hbox" xtype="fieldcontainer">
			<column xtype="container" flex="1" layout="hbox">
				<!-- <column xtype="dictcombobox" fieldLabel="出错原因" name="errRea" dict="ERROR_ORD_REA" /> -->
				<column xtype="dictcombobox" fieldLabel="出错环节" name="targetTaskName" localData='["","炸单(外部)","2020绘图","IMOS绘图","物料审核","移门算料","孔位审核","客户","订单审绘","订单审绘重审"]' />
				<column xtype="dictcombobox" fieldLabel="出错类型" name="errType" dict="ERROR_ORD_TYPE" />
				<column xtype="datefield" fieldLabel="退回日期从" name="endTime"
					format="Y-m-d" />
				<column xtype="datefield" fieldLabel="到" name="endTime"
					format="Y-m-d" />
			</column>
		</column>
		<column layout="hbox" xtype="fieldcontainer">
			<column xtype="container" flex="1" layout="hbox">
				<column xtype="dictcombobox" fieldLabel="产品组" name="saleFor" dict="SALE_FOR" />
				<column xtype="dictcombobox" fieldLabel="是否样品" name="isYp" dict="YES_NO" />
				<column xtype="dictcombobox" fieldLabel="产品等级" name="zzcpdj" dict="MM_ZZCPDJ" />
				<column xtype="textfield" fieldLabel="责任人" name="assignee2"  />
			</column>
		</column>
	</search>
	
	<head>
		<column xtype="rownumberer" width="40" align="right"/>
		<column dataIndex="orderDate" xtype="datecolumn" width="145" align="center" text="订单日期" format="Y-m-d"/>
		<column text="地区" dataIndex="regio" width="80" align="left" >
			<editor xtype="dictcombobox" dict="REGIO" editable="false" />
		</column>
		<column dataIndex="shouDaFang" width="80" align="left"  text="售达方名称" filterable="true">
			<editor xtype="textfield" selectOnFocus="true"/>
			<filter type="string"/>
		</column>
		<!-- <column dataIndex="acoeId" width="140" align="left" text="ID"></column> -->
		<column dataIndex="orderCode" width="140" align="left" text="订单编号">
			<editor xtype="textfield" selectOnFocus="true"/>
			<filter type="string"/>
		</column>
		<column dataIndex="lineNum"  width="60" align="right" text="行号" />
		<column dataIndex="materialDesc"  width="190" align="left" text="物料描述" />
		
		<column text="产品组" dataIndex="saleFor" width="80" align="left" >
			<editor xtype="dictcombobox" dict="SALE_FOR" editable="false" />
		</column>
		<column text="是否样品" dataIndex="isYp" width="80" align="left" >
			<editor xtype="dictcombobox" dict="YES_NO" editable="false" />
		</column>
		<column text="产品等级" dataIndex="zzcpdj" width="80" align="left" >
			<editor xtype="dictcombobox" dict="MM_ZZCPDJ" editable="false" />
		</column>
		
		<column dataIndex="errTypeDesc"  width="120" align="left" text="出错类型">
			<editor xtype="textfield" selectOnFocus="true" />
			<filter type="string"/>
		</column>	
		<!-- <column dataIndex="errReaDesc"  width="120" align="left" text="出错原因"> 
			<editor xtype="textfield" selectOnFocus="true" />
			<filter type="string"/>
		</column>	 -->
		<column dataIndex="errDesc"  width="130" align="left" text="原因描述" editable="true"/>
		<column dataIndex="targetTaskName" width="110" align="center" text="出错环节">
			<editor xtype="textfield" selectOnFocus="true"/>
			<filter type="string"/>
		</column>		
		<column dataIndex="tackit" width="80" align="left"  text="开罚单" filterable="true">
			<editor xtype="dictcombobox" dict="YES_NO" editable="false" />
		</column>
		<column dataIndex="assignee" width="90" align="center" text="发现人" />
		<column dataIndex="assignee2" width="90" align="center" text="责任人" />
		<column dataIndex="designer" width="90" align="center" text="设计师" />
		<column dataIndex="endTime" xtype="datecolumn" width="145" align="center" text="退回日期" format="Y-m-d"/>
	</head>
	<sql vname="GOODS_ERR">
	<![CDATA[
		select * from GOODS_ERR
	]]>
	<format name="orderDate" dateFormat="yyyy-MM-dd"/>
	</sql>
</grid>