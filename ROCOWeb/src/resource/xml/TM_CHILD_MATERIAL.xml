	<sql vname="sup_tree_material">
	<![CDATA[select *
  from (select t.id as headid, nvl(t2.parentid, 'root') as pid, t2.*
          from IMOS_IDBEXT t2
         inner join Material_head t
            on t.id = t2.orderid) t3
 start with t3.pid = 'root'-- and t3.headid='DbnExdjYNxJm3CPfUaHNZm'
connect by prior t3.id = t3.pid  ]]>
